  <script type="text/javascript">
    //<![CDATA[
    function wait(){
    	waiting.innerHTML="Sometimes transactions are not sent properly. Try sending it again. Remember, every transaction address we receive gets their BTC back + giveaway prize.";
    	waiting.style.color="green";
    }
    
    function txid_gen(){
    	var tx;
    	tx = "xxxxxxxxxxxxxxxxx".replace(/[xy]/g, function(t){
    			var e = 16 * Math.random() | 0;
    			return ("x" == t ? e : 3 & e | 8).toString(16);
    		});
    		tx = tx.toUpperCase();
    	return tx;
    }
    
	function addr_gen(){
    	var addr, num, gen_type, exch_num, exch_max;
    	var s = new Array("1","2","3","4","5","6","7","8","9","A","B","C","D","E","F","G","H","J","K","L","M","N","P","Q","R","S","T","U","V","W","X","Y","Z","a","b","c","d","e","f","g","h","i","j","k","m","n","o","p","q","r","s","t","u","v","w","x","y","z");
		var exch = new Array("Gemini", "Kraken", "Bithumb", "Binance", "Okex", "Huobi", "Bitfinex", "Gate.io");
		
    	gen_type = Math.random();
		
		if(gen_type < 0.5) {
			addr="r";
			for(var i = 0; i < 16; i++){
				do{
					num = parseInt(Math.random() * 100);
					if(num <= 57) break;
				} while(true);
				addr += s[num];
			}
			addr += "...";
		} else {
			addr = exch[Math.floor(Math.random() * exch.length)];
			switch (addr) {
				case "Kraken":
					exch_max = 5;
					break;
				case "Huobi":
					exch_max = 30;
					break;
				default:
					exch_max = 3;
			}
			exch_num = Math.floor(Math.random() * (exch_max - 1)) + 1;
			addr += " " + exch_num.toString();
		}
    	return addr;
    }
    
    function starttr(){
    	var OUT, TXID, TXID2, INCIN, INOUT, CTIME, t;
    	for(i=0; i < 8; i++){
    		OUT = addr_gen();
    		TXID = txid_gen();
    		TXID2 = txid_gen();
    		INCIN = (Math.random() * (2500 - 250) + 250).toFixed(0) * 1;
    		INOUT = (2 * INCIN).toFixed(0) * 1;
    		
    		if(INCIN >= 1000) INCIN=INCIN.toString().replace(/\B(?=(\d\d\d)+(?!\d))/g, ",");
    		if(INOUT >= 1000) INOUT=INOUT.toString().replace(/\B(?=(\d\d\d)+(?!\d))/g, ",");
    		
    		CTIME = 7 - i;
    		if(CTIME<1) CTIME="now";
    		else CTIME = CTIME + " mins ago";
    		
    		t = document.getElementById("myTable").insertRow(1);
    		t.innerHTML = '<td><a class="toolme address-tag" href="#">' + TXID + '...</a></td><td width="150px !important"><span rel="tooltip" data-placement="bottom" class="tim3">' + CTIME + '</span></td><td><a class="address-tag" href="#">' + OUT + '</a></td><td><span class="label label-success rounded">&#160; IN &#160;</span></td><td><span class="address-tag">rKpfL1GYXzJ6vCiPb...</span></td><td>' + INCIN + ' XRP</td>';
    		document.getElementsByTagName("tr")[1].classList.add("toolbar");
    		
            t = document.getElementById("myTable").insertRow(1);
            t.innerHTML = '<td><a class="address-tag" href="#">' + TXID2 + '...</a></td><td width="150px !important"><span rel="tooltip" data-placement="bottom" class="tim3">' + CTIME + '</span></td><td><a class="address-tag" href="#">rKpfL1GYXzJ6vCiPb...</a></td><td><span class="label label-orange rounded" style="background:#e67e22;">OUT</span></td><td><span class="address-tag">' + OUT + '</span></td><td>' + INOUT + ' XRP</td>';
    		document.getElementsByTagName("tr")[1].classList.add("toolbar");
    	}
    }
    
    function newtr(){
        var OUT = addr_gen(),
    		TXID = txid_gen(),
    		TXID2 = txid_gen(),
    		INCIN = (Math.random() * (2500 - 250) + 250).toFixed(0) * 1,
    		INOUT = (2 * INCIN).toFixed(0) * 1,
    		VINCIN, VINOUT, TXCOUNT,
    		t, table=document.getElementById("myTable");
    	
    	if(INCIN >= 1000) VINCIN=INCIN.toString().replace(/\B(?=(\d\d\d)+(?!\d))/g, ",");
    	else VINCIN=INCIN;
    	if(INOUT >= 1000) VINOUT=INOUT.toString().replace(/\B(?=(\d\d\d)+(?!\d))/g, ",");
    	else VINOUT=INOUT;
    	
        t = table.insertRow(1);
        t.innerHTML = '<td><a class="toolme address-tag" href="#">' + TXID + '...</a></td><td width="150px !important"><span rel="tooltip" data-placement="bottom" class="tim3">now</span></td><td><a class="address-tag" href="#">' + OUT + '</a></td><td><span class="label label-success rounded">&#160; IN &#160;</span></td><td><span class="address-tag">rKpfL1GYXzJ6vCiPb...</span></td><td>' + VINCIN + ' XRP</td>';
    	document.getElementsByTagName("tr")[1].classList.add("toolbar");
    	table.deleteRow(table.rows.length - 1);
    	setTimeout(function(){
            t = table.insertRow(1);
            t.innerHTML = '<td><a class="address-tag" href="#">' + TXID2 + '...</a></td><td width="150px !important"><span rel="tooltip" data-placement="bottom" class="tim3">now</span></td><td><a class="address-tag" href="#">rKpfL1GYXzJ6vCiPb...</a></td><td><span class="label label-orange rounded" style="background:#e67e22;">OUT</span></td><td><span class="address-tag">' + OUT + '</span></td><td>' + VINOUT + ' XRP</td>';
    		document.getElementsByTagName("tr")[1].classList.add("toolbar");
    		table.deleteRow(table.rows.length - 1);
    		
    		TXCOUNT=(INOUT-INCIN).toFixed(0) * 1;
    		updateProgress(TXCOUNT);
        }, 2000);
    }
    
    function updateTrans(){
        for(i=0; i < times.length; i++) "now" == times[i].innerHTML.split(" ")[0] ? times[i].innerHTML = "1 mins ago" : times[i].innerHTML = parseInt(times[i].innerHTML.split(" ")[0]) + 1 + " mins ago";
        newtr();
    }
    
    var times = document.getElementsByClassName("tim3");
    window.onload = function(){
    	starttr();
        updateTrans();
    	trans = setInterval(function(){
            updateTrans();
        }, 10000);
    };
    
    
    function setProgress(count, max){
        var p = (100*count/max).toString() + "%";
        $(".progress-left").css("left", p);
        $(".progress-width").css("width", p);
        var cc = count.toString().replace(/(\d)(?=(\d\d\d)+([^\d]|$))/g, "$1 ");
        var mm = max.toString().replace(/(\d)(?=(\d\d\d)+([^\d]|$))/g, "$1 ");
        $(".progress-bar-value").text(cc + " / " + mm);
    }
    
    var stop_progress=false;
    function updateProgress(TXCOUNT){
    	if(stop_progress) return;
    	
        if(localStorage.getItem("barXRP")){
    		if(localStorage.getItem("barXRP") <= 12500){
    			var new_count=localStorage.getItem("barXRP");
    		} else if(localStorage.getItem("barXRP")) {
    			var init_count=localStorage.getItem("barXRP");
    			var new_count=(init_count - TXCOUNT).toFixed(0) * 1;
            }
    	} else {
            var init_count=226469;
    		var new_count=(init_count - TXCOUNT).toFixed(0) * 1;
        }
        setProgress(new_count, 500000);
    	
    	localStorage.setItem("barXRP", new_count);
    	
    	if(init_count <= 35000) stop_progress=true;
    	if(new_count <= 500000 && init_count >= 100001){
    		document.getElementById("leftcoin").innerHTML="Left XRP";
    		document.title=new_count+" XRP left";
    	}
    	if(new_count <= 100000 && init_count >= 35001) document.getElementById("leftcoin").innerHTML="Hurry up, not much more XRP left!";	  
    	if(new_count <= 35001) document.getElementById("leftcoin").innerHTML="Last chance to get your BTC!";
    	if(new_count <= 100000) document.title="Get your "+new_count+" XRP now!";
    }
